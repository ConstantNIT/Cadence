
// Vermix stimulus file.
// Default verimix stimulus. 


real start;
real delay;
initial
begin 

   a0[21:0] = 22'b0000000000000000000000;
   a1[21:0] = 22'b0000000000000000000000;
   b0[21:0] = 22'b0000000000000000000000;
   b1[21:0] = 22'b0000000000000000000000;

#500;
   
   a0=$random;
   a1=~a0;
   b0=a0;
   b1=a1;
   start=$time;
   wait(Valid);

   delay=$time-start;
   a0[21:0] = 22'b0000000000000000000000;
   a1=a0; b0=a0; b1=a1;
   wait(~Valid);
$finish;

end 
