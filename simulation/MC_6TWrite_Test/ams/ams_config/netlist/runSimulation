incrementalElab=no

# Elaborate '16nm_Tests' 'MC_6TWrite_Test' 'ams_config'
elab '16nm_Tests' 'MC_6TWrite_Test' 'ams_config' '\16nm_Tests ' 'cds_globals' 'MC_6TWrite_Test_ams_config' '' '' '' ncelab -cds_implicit_tmpdir ${AMS_NETLIST_DIR}/ihnl -timescale 1ps/1ps -discipline logic -nowarn DLNOHV -delay_mode None -novitalaccl -access +r -errorMax 50 -messages -status -v93 -cdslib ${AMS_CDS_LIB} -noparamerr -use5x4vhdl -update -amspartinfo ${AMS_RESULTS_DIR}/partition.info -rnm_partinfo -modelpath "${AMS_INVOKE_DIR}/ncsu-FreePDK15/PTM-MG/smodels.scs" -work 16nm_Tests '16nm_Tests.MC_6TWrite_Test:ams_config' connectLib.ConnRules_18V_full_fast:connect 16nm_Tests.cds_globals:MC_6TWrite_Test_ams_config -logfile ${AMS_RESULTS_DIR}/ncelab.log 

# Simulate '16nm_Tests' 'MC_6TWrite_Test' 'ams_config'
ncsim  -cds_implicit_tmpdir ${AMS_NETLIST_DIR}/ihnl -analogControl ${AMS_NETLIST_DIR}/amsControlSpectre.scs -unbuffer -nowarn DLNOHV -errorMax 50 -messages -status -cdslib ${AMS_CDS_LIB} -input ${AMS_NETLIST_DIR}/amsControl.tcl -run -exit 16nm_Tests.MC_6TWrite_Test:ams_config -logfile ${AMS_RESULTS_DIR}/ncsim.log +amsrawdir ${AMS_RESULTS_DIR}  -simcompat spectre 

convertReturnValue $? 0

setReturnValue $? 'Successfully simulated ("16nm_Tests" "MC_6TWrite_Test" "ams_config").' 'Failed to simulate ("16nm_Tests" "MC_6TWrite_Test" "ams_config").'
