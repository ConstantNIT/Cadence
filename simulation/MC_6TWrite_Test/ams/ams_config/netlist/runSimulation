compile 'connectLib' 'E2R_LPS' 'module' "${AMS_AMS_HIER}/tools.lnx86/affirma_ams/etc/connect_lib/connectLib/E2R_LPS/module/verilog.vams" ncvlog -cds_implicit_tmpdir ${AMS_NETLIST_DIR}/ihnl -cds_implicit_tmponly -use5x -ams  -work 'connectLib' -specificunit 'E2R_LPS'  -view 'module'  -errormax 50 -update -uptodate_messages -cdslib ${AMS_CDS_LIB} -log ${AMS_RESULTS_DIR}/ncvlog.log -append_log  -incdir ${AMS_INVOKE_DIR}/ -incdir ${AMS_NETLIST_DIR} -messages -status -nowarn DLNOHV "${AMS_AMS_HIER}/tools.lnx86/affirma_ams/etc/connect_lib/connectLib/E2R_LPS/module/verilog.vams"

